{
	"name": "markdownspace-plugin",
	"version": "1.0.0",
	"description": "This is a markdownspace plugin for automatically publishing documents to markdownspace",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"markdownspace",
		"website",
		"website builder"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@tsconfig/svelte": "^5.0.2",
		"@types/node": "^20.10.8",
		"@typescript-eslint/eslint-plugin": "6.18.1",
		"@typescript-eslint/parser": "6.18.1",
		"builtin-modules": "3.3.0",
		"esbuild": "0.19.11",
		"esbuild-svelte": "^0.8.0",
		"obsidian": "latest",
		"svelte": "^4.2.8",
		"svelte-preprocess": "^5.1.3",
		"tslib": "2.6.2",
		"typescript": "5.3.3"
	},
	"dependencies": {
		"@markdownspace/api": "^1.0.9",
		"@tabler/icons": "^2.45.0"
	}
}
